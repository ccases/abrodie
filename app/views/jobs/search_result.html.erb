<head>
  <link href="https://fonts.googleapis.com" rel="preconnect"></link>
  <link href="https://fonts.gstatic.com" rel="preconnect"></link>
  <link href="https://fonts.googleapis.com/css2?family=Mulish:wght@300&family=Poppins&family=Raleway:wght@500&family=Roboto&display=swap" rel="stylesheet"></link>
</head>

<div class= 'jobsandagencies-wrapper'>
    <div class='jobsandagencies-searchbar'
        <%= form_tag(search_result_path, :method => "get", local: false) do %>
            <span>
                <ion-icon name="search"></ion-icon>
            </span>
            <%= search_field_tag :job, params[:job] %>
            <%= button_tag 'FIND JOB', class: 'find-job-btn',:name => nil %>
        <% end %>
    </div>
    <div id='search_result'>
        <%= render 'jobs/search_result' %>
    </div>
</div>